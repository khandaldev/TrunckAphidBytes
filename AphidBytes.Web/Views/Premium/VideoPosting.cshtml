            socialarray.push("Dailymotion");
        }
        if ($("#YouTube").is(':checked')) {
            socialarray.push("YouTube");
        }
        if ($("#Twitter").is(':checked')) {
            socialarray.push("Twitter");
        }
        if (i < socialarray.length) {
            var val = socialarray[i];
            i++;
            return val;
        }
        return "";
    }


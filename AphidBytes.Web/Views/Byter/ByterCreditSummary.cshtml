    function CheckBoxClick(classname) {


        var clas = classname;
        var arr = [];
        arr = clas.split("");
        for (var i = 0; i < arr.length; i++) {
            if ($.isNumeric(arr[i])) {
                break;
            }
        }
        var dat = classname.substring(i, arr.length);
        if ($("#" + dat).is(':checked')) {

            var clas = classname;
            var selected = $("#" + dat).val();

            $.post('/Byter/ShowNetwork', { Selected: selected }, function (data) {
                for (var i = 0; i < data.length; i++) {
                    $("#" + clas).append("<div class='ImgSel" + i + "'><img id='imgid" + i + "' onclick='ChooseItem(" + i + ")' src=" + data[i].Path + " width='20px' height='20px'></img><input type='checkbox' name='genre' id='check-1' value=" + data[i].Id + " onclick=" + "SelectedNetwork(); /></div>");

                }
            });

        }
        else {
            document.getElementById(clas).innerHTML = '';
        }
    }

    function categorychange(key) {
        if (key == 'Music') return 0;
        if (key == 'Video') return 1;
        if (key == 'Photos') return 2;
        if (key == 'Pdf') return 3;
        if (key == 'Files') return 4;

    }

    function checklist(id) {
        $("#searchbtn").attr('class', id);
        var type;
        var body = "";
        var postedbody = "";

        if (id == "one") {
            $('#sub').show();
            $(".posteddatatable").remove();
            $(".postedfetchdata").remove();
            $.post('/Byter/GetPostedDataResult', {}, function (data) {
                if (data != null) {

                    for (var i = 0; i < data.length; i++) {
                        posteddata = null;
                        postedbody = "<tr class='posteddatatable'>";

                        postedbody += " <td><p>" + data[i].Title + "</p></td>";
                        postedbody += "<td align='center'><p>" + data[i].Category + "</p></td>";
                        postedbody += " <td align='center'><p>" + data[i].Credit + "</p></td></td>";
                        postedbody += "<td><p>" + data[i].Composer + "</p></td>";
                        postedbody += " <td><p>" + data[i].TrackingId + "</p></td>";
                        postedbody += "</tr>";
                        $(".postedfetchdata").remove();
                        $("#posteddata").append(postedbody);
                    }

                }

            });
        }



        if (id == "two") {
            $('#sub').hide();
            $.post('/Byter/GetPostQueueData', {}, function (data) {
                $(".newData").remove();
                for (var i = 0; i < data.length; i++) {

                    if (i % 2 == 0) {

                        body = "<tr class='newData'>";
                        body += "<td class='gradient-tr1'><p>" + data[i].Composer + "</p></td>";

            });
        }
        else if (id == "three") {
            var bodyM = "";
            $(".Mdata").remove();
            $('#sub').show();
            $.post('/Byter/GetLinkToPostM', {}, function (data) {

                for (var i = 0; i < data.length; i++) {

                    if (i % 2 == 0) {
                        bodyM = "<tr class='Mdata'>";
                        bodyM += "<td class='table-tha'><input type='checkbox' name='genre' class='check-1' id=" + i + " value=" + data[i].TrackingId + ',' + data[i].Category + " onclick=" + "CheckBoxClick('Social" + i + "'); />";
                        bodyM += " <label for='check-1'></label>";
                        bodyM += " </td>";
                        bodyM += "<td class='table-trb3'><p>" + data[i].Composer + "</p></td>";

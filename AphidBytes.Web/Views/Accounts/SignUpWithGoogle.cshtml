    function profilechange() {
        var name = $('#ProfilePicture').val();
        var profile = $('#fbpic').attr('src');
        //alert(profile);
        if (name != '')
        {
           
            var split = name.split('.');
            if (split[1] == "jpg" || split[1] == "JPG" || split[1] == "png" || split[1] == "PNG") {
                $('#picture').text(" ");
            }
            else {

                $("#picture").show();
                $('#picture').attr('style', 'color:#ffc000; width: 87px;');
                $('#picture').text("Invalid Image");
                $("#ProfilePicture").val(" ");
            }
        }
        else if (profile != '')
        {
            //$("#fbpic").attr('src');
        }
        else {
            $('#picture').attr('style', 'color:#ffc000;width:158px;');
            $('#picture').text('Required');
        }

    }
    //});
    function Cardchange() {
        var ame_regex = /^(?:3[47][0-9]{13})$/;
        var visa_regex = /^(?:4[0-9]{12}(?:[0-9]{3})?)$/;
        var master_regex = /^(?:5[1-5][0-9]{14})$/;
        var cardno = $('#CardNumber').val();
        if (cardno != "") {
            if (cardno.match(ame_regex)) {
                res = "American Express";
                //$('#card').attr('style', 'color:red;width:8px;');
                $('#card').text(" ");
            }
            else if (cardno.match(visa_regex)) {
                res = "Visa";
                //$('#card').attr('style', 'color:red;width:8px;');
                $('#card').text(" ");
            }

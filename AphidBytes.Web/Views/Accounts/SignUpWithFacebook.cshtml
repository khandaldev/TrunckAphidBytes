    function password_Change() {
        var pass = $('#Password').val().trim();
        var pass1 = $('#pass').text();
        if (pass == "") {
            $('#pass').attr('style', 'color:#ffc000;width:149px');
            $('#pass').text('Required');
        }
        else {
            var count = pass.length;
            if (count < 8) {
                $('#pass').attr('style', 'color:#ffc000;width:149px');
                $('#pass').text('Atleast 8 characters');
            }
            else {
                $('#pass').text(" ");
            }
        }
    }
    function cnfrm_password() {
        var pass = $('#Password').val().trim();
        var cpass = $('#ConfirmPassword').val().trim();
        //var cpass1 = $('#cpass').text();
        if (cpass == "") {
            $('#cpass').attr('style', 'color:#ffc000;width:149px');
            $('#cpass').text('Required');
        }
        else {
            if (pass == cpass) {
                $('#cpass').attr('style', 'color:#ffc000;width:149px');
                $('#cpass').text(" ");
            }
            else {
                $('#cpass').attr('style', 'color:#ffc000;width:149px');
                $('#cpass').text('Password not match');
            }
        }

    }

<script type="text/javascript">
    function openpopup(v) {
        if ($("#MatrixImage").val() != '') {
            $("#ImgClose").show();
            $("#popup:visible").hide(); //hide popup if it is open
            // e.preventDefault(); // don't follow link   "~/TempBasicImages/
            var valk = "/videobgp/" + v;
            document.getElementById('image-placeholder').src = valk;
            $("#image-placeholder").attr("src", $(this).attr("href")); // replace image src with href from the link that was clicked
            $("#popup").fadeIn("fast"); //show popup
            //$("#ImgClose").hide();
            $("#imgpre").attr('src', '/TempBasicImages/' + fileop);
        }
        else {

            $("#button-bg2").dialog("open");

        }

    }

    function CloseThis() {
        $("#ImgClose").hide();

    }
    $("#popup-close").click(function (e) {
        e.preventDefault();
        $("#popup").fadeOut("fast");
    });
    function showPopup() {
        var div = document.createElement('div');

        div.className += 'popup';
        div.innerHTML = 'This is a pop up dialog';
        document.body.appendChild(div);
    }

    $(function () {
        $('#button-bg').hide();
        
    }); 

    
</script>

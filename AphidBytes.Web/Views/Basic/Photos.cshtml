    var width = 400;
    var height = 250;
    function zoomin() {

        var new_width = width + 30 + "px";
        var new_height = height + 30 + "px";
        if (new_width <= "570px") {
            //alert(new_width);
            $("#imgpre").css('width', new_width);
            $("#imgpre").css('height', new_height);
            width = width + 30;
            height = height + 30;
        } else {
            $("#button-bg4").dialog("open");
           // alert("Maximun Zoom");
        }
    }

    function zoomout() {
        if (width > 450) {
            new_width = width;
            new_height = height;
            var less_width = new_width - 30 + "px";
            var less_height = new_height - 30 + "px";
            if (less_width >= "330px") {

                $("#imgpre").css('width', less_width);
                $("#imgpre").css('height', less_height);
                width = width - 30;
                height = height - 30;
            }
            else {
                alert("Minimum Zoom");
            }
        }

        else {
            width = 400;
            height = 250;
            var less_width = width - 30 + "px";
            var less_height = height - 30 + "px";
            if (less_width >= "250px") {

                $("#imgpre").css('width', less_width);
                $("#imgpre").css('height', less_height);
                width = width - 30;
                height = height - 30;
            } else {

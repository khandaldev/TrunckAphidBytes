<script>
    $(document).ready(function () {

        $('#upgradePremium').click(function () {
            $("#yesno").attr('style', 'display:block');
        });

        $("#yes").click(function () {
            $("#yesno").attr('style', 'display:none');
            $.post('/Basic/UpgardeToPremium', {}, function (data) {
                if (data == "Success") {

                    $("#upgradebox").attr('style', 'display:block');
                } else { $("#button-bg3").attr('style', 'display:block'); }

            });

        });
        $("#no").click(function () {
            $("#yesno").attr("style", "display:none");
        });

    });
    function Yes_Button() {
        var value = $('#imgID').attr('src');
        if (value == "/Images/ficone.png") {
            fb_posting();

        }
        if (value == "/Images/linkdicone.png") {
            link_posting();
        }
        if (value == "/img/youticone.png") {
            youtube_posting();
        }
        if (value == "/img/vimeoicone.png") {
            dailymotion_posting();
        }
        if (value == "/Images/ticone.png")
            tw_posting();
    }
    function No_Button() {
        $("#pop_wrapper").hide();
        $(".pop_content").hide();
        var data = social();
        if (data == "Facebook") {
            fb_posting();
        }
        else if (data == "Linkedin") {
            link_posting();
        }
        else if (data == "DailyMotion") {
            dailymotion_posting();
        }
        else if (data == "YouTube") {
            youtube_posting();
        }
        else if (data == "Twitter") {
            tw_posting();
        }

        else {
            $("#pop_wrapper").hide();
            $(".pop_content").hide();
           
        }
    }


    (function ($) {

        $("#browseBtns,#browseBtnsAdd").fadeTo('slow', .2);
        $(".browseInput").each(function (index) {
            if (index > 3)
                $(this).remove();
        });
        $("#browseBtns,#browseBtnsAdd,#btnUpload").prop('disabled', true);

        $(window).load(function () {
            $(".content").mCustomScrollbar();
        });
    })(jQuery);
</script>

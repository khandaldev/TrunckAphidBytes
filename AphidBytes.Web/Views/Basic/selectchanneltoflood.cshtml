<script type="text/javascript">


    function Fileprivew(trackingnumber) {
        var ii = "Video";

        $.post('/Basic/FilePrivew', { Trackingnumber: trackingnumber, Category: ii }, function (data) {
            //alert("data");
            window.location = data;
        });
    }
    function tw_posting() {
        $.post('@Url.Action("TwitterPost", "Basic")', { Type: "Videos" }, function (data) {
            if (data == "Twitter") {
                document.getElementById("buttonbg2").style.display = "block";
                $('#socialname1').text("Already post on Twitter");
            }
            if (data == "login error") {
                location.href = "/Accounts/LoginUser";
            }
            if (data == "Linkpost") {
                window.location = data;
            }
            else if (data == "Index") {
                location.href = "/SocialNetworks/Index";
            }
            else if (data == "Successfull") {
                document.getElementById("Popup1").style.display = "block";
                $("#imgsocial").attr('src', '/img/ticone.png');
             
            }
            else if ((data != "Successfull") && (data != "Twitter")) {
                $("#imgID").attr('src', '/img/ticone.png');
                $("#pop_wrapper").show();
                $(".pop_content").show();

            }
        });
    }

